{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictFunctionTypes": false,
        "esModuleInterop": true,
        "lib": [
            "es2018",
            "dom"
        ],
        "resolveJsonModule": true,
        "outDir": "dist",
        "sourceMap": true,
        "rootDir": "src"
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ],
    
}